<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>AlphaSound.Protocol</RootNamespace>
    <PackageTags>minecraft bedrock</PackageTags>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Authors>AlphaSound</Authors>
    <Company>AlphaSound</Company>
    <Description>A Protocol Library for Minecraft Bedrock Edition</Description>
    <PackageProjectUrl>https://github.com/AlphaSound/Protocol</PackageProjectUrl>
    <RepositoryUrl>https://github.com/AlphaSound/Protocol</RepositoryUrl>
    <Version>0.1.0</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
